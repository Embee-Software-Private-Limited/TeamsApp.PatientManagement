    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "*.azurewebsites.net"
    ],
    "webApplicationInfo": {
        "id": "<AppId>",
        "resource": "api://<AppDomain>/botid-<AppId>"
